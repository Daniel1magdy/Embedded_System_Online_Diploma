#include<stdio.h>

void main (void)
{
	int x , y ,z ;
	printf("Please enter an the vaue of the first number  : ");
	fflush(stdin); fflush(stdout);
	scanf("%d",&x);

	printf("Please enter an the vaue of the second number  : ");
	fflush(stdin); fflush(stdout);
	scanf("%d",&y);

	printf("Please enter an the vaue of the third number  : ");
	fflush(stdin); fflush(stdout);
	scanf("%d",&z);



	if((x > y) && (x > z))
	{
		printf("Largest number is %d",x);
	}
	if((y > x) && (y > z))
	{
		printf("Largest number is %d",y);
	}
	if((z > x) && (z > y))
	{
		printf("Largest number is %d",z);
	}

}

